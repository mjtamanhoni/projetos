  object FDQRegistros: TFDQuery
    Connection = DM_Global.FDC_Firebird
    SQL.Strings = (
      'SELECT '
      #9'C.TIPO'
      #9',C.ID'
      #9',C.NOME'
      #9',C.PESSOA '
      #9',C.DOCUMENTO '
      #9',C.INSC_EST '
      #9',C.ENDERECO '
      #9',C.COMPLEMENTO '
      #9',C.NUMERO '
      #9',C.BAIRRO '
      #9',C.CIDADE '
      #9',C.UF '
      #9',C.TELEFONE '
      #9',C.CELULAR '
      #9',C.EMAIL'
      #9',C.ID_TAB_PRECO '
      #9',CASE C.PESSOA'
      #9#9'WHEN 0 THEN '#39'F'#205'SICA'#39
      #9#9'WHEN 1 THEN '#39'JUR'#205'DICA'#39
      #9'END PESSOA_DESC'
      'FROM (SELECT'
      #9#9#9'  '#39'CLIENTE'#39' AS TIPO'
      #9#9#9#9',C.ID'
      #9#9#9#9',C.NOME'
      #9#9#9#9',C.PESSOA '
      #9#9#9#9',C.DOCUMENTO '
      #9#9#9#9',C.INSC_EST '
      #9#9#9#9',C.ENDERECO '
      #9#9#9#9',C.COMPLEMENTO '
      #9#9#9#9',C.NUMERO '
      #9#9#9#9',C.BAIRRO '
      #9#9#9#9',C.CIDADE '
      #9#9#9#9',C.UF '
      #9#9#9#9',C.TELEFONE '
      #9#9#9#9',C.CELULAR '
      #9#9#9#9',C.EMAIL'
      #9#9#9#9',C.ID_TAB_PRECO '
      #9#9#9'FROM CLIENTE C'
      #9#9#9'UNION ALL'
      #9#9#9'SELECT'
      #9#9#9#9#39'FOENECEDOR'#39' AS TIPO'
      #9#9#9#9',F.ID'
      #9#9#9#9',F.NOME'
      #9#9#9#9',F.PESSOA '
      #9#9#9#9',F.DOCUMENTO '
      #9#9#9#9',F.INSC_EST '
      #9#9#9#9',F.ENDERECO '
      #9#9#9#9',F.COMPLEMENTO '
      #9#9#9#9',F.NUMERO '
      #9#9#9#9',F.BAIRRO '
      #9#9#9#9',F.CIDADE '
      #9#9#9#9',F.UF '
      #9#9#9#9',F.TELEFONE '
      #9#9#9#9',F.CELULAR '
      #9#9#9#9',F.EMAIL'
      #9#9#9#9',0 AS ID_TAB_PRECO '
      #9#9#9'FROM FORNECEDOR F) C'
      'WHERE NOT C.ID IS NULL'
      'ORDER BY 1,2;'
      '')
