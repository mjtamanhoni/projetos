  object FDQRegistros: TFDQuery
    Connection = DM_Global.FDC_Firebird
    SQL.Strings = (
      'SELECT '
      #9'CP.*'
      #9',CASE CP.TIPO_INTERVALO'
      #9#9'WHEN 0 THEN '#39'DIAS'#39
      #9#9'WHEN 1 THEN '#39'MESES'#39
      #9'END TIPO_INTERVALO_DESC'
      'FROM CONDICAO_PAGAMENTO CP'
      'WHERE NOT CP.ID IS NULL '
      'ORDER BY '
      #9'CP.ID;'
