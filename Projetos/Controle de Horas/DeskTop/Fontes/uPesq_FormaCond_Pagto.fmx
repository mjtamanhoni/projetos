  object FDQRegistros: TFDQuery
    Connection = DM_Global.FDC_Firebird
    SQL.Strings = (
      'SELECT '
      #9'FCP.* '
      #9',FP.DESCRICAO AS FORMA_PAGAMENTO'#9
      #9',CP.DESCRICAO AS CONDICAO_PAGAMENTO'
      'FROM FORMA_CONDICAO_PAGAMENTO FCP'
      #9'JOIN FORMA_PAGAMENTO FP ON FP.ID = FCP.ID_FORMA_PAGAMENTO '
      
        #9'JOIN CONDICAO_PAGAMENTO CP ON CP.ID = FCP.ID_CONDICAO_PAGAMENTO' +
        ' '
      'WHERE NOT FCP.ID IS NULL '
      'ORDER BY '
      #9'FCP.ID_FORMA_PAGAMENTO '
      #9',FCP.ID_CONDICAO_PAGAMENTO;'
      '')
